{
  "name": "d3-react",
  "version": "1.0.0",
  "description": "Slideshow using d3 & react",
  "main": "index.js",
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --mode development --open --hot",
    "clean-dist": "rimraf dist/*",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "test": "cross-env NODE_ENV=test npm run lint && jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --ignore-path .gitignore -- .",
    "lint:css": "stylelint './src/**/*.js'",
    "stats": "webpack --profile --json > stats.json && mv stats.json dist && webpack-bundle-analyzer dist/stats.json"
  },
  "author": "peter@peterorum.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/peterorum/d3-react"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.4.3",
    "@babel/plugin-transform-react-constant-elements": "^7.2.0",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "clean-webpack-plugin": "^2.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-compat": "^3.1.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "jest-dom": "^3.1.3",
    "jest-fetch-mock": "^2.1.2",
    "jest-styled-components": "^6.3.1",
    "prettier": "^1.17.0",
    "prop-types": "^15.7.2",
    "react-testing-library": "^6.1.2",
    "style-loader": "^0.23.1",
    "stylelint": "^10.0.1",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.6.0",
    "webpack": "^4.30.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "grommet": "^2.6.6",
    "grommet-icons": "^4.2.0",
    "immer": "^3.1.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.2",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2",
    "reselect": "^4.0.0",
    "sanitize.css": "^8.0.0",
    "styled-components": "^4.2.0",
    "whatwg-fetch": "^3.0.0"
  }
}
